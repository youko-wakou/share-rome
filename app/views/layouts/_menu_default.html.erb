<div>
    <nav class="navbar container-fluid" id="head-menu">
        <%= link_to root_path do %>
            <%= image_tag 'title.png',:class => 'col-md-4 col-sm-6 col-xs-10',:id => 'title_img' %>
        <% end %>
        <div class="col-md-4 col-sm-5 hidden-xs col-md-offset-3 col-sm-offset-1" id="top-login-menu">
           <ul class="nav navbar-nav">
               <% unless logged_in? %>
                   <li><%= link_to 'ログイン',login_path %></li>
                   <li><%= link_to 'サインアップ',signup_path %></li>
               <% else %>
                   <li><%= link_to 'ログアウト',logout_path,method: :delete %></li>
               <% end %>
               
               
           </ul>
        </div>
    </nav>
</div>